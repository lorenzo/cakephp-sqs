{
    "name": "lorenzo/cakephp-sqs",
    "description": "CakePHP Aws Simple Queue System",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "queue", "aws", "sqs"],
    "homepage": "https://github.com/lorenzo/cakephp-sqs",
    "license": "MIT",
    "authors": [
        {
            "name": "José Lorenzo Rodríguez",
            "role": "Author"
        },
        {
            "name": "Cake Development Corporation",
            "role": "Contributor"
        }
    ],
    "require": {
        "cakephp/cakephp": "^4.0.0",
        "aws/aws-sdk-php": "~3.32" 
    },
    "require-dev": {
        "phpunit/phpunit": "8.*",
        "php-vcr/php-vcr": "dev-fix-stream-emulation as 1.3.3",
        "php-vcr/phpunit-testlistener-vcr": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "CakeSQS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeSQS\\Test\\": "tests",
            "CakeSQS\\Test\\Fixture\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/markstory/php-vcr"
        }
    ]
}




